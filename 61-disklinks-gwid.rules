# map drives to GWID

ENV{DEVTYPE}=="disk|partition", ENV{ID_SERIAL}=="TOSHIBA_HDWQ140_39UHK106FAYG", ENV{ID_GWID}="GWHD009"
ENV{DEVTYPE}=="disk|partition", ENV{ID_SERIAL}=="TOSHIBA_HDWQ140_39VIK0QRFAYG", ENV{ID_GWID}="GWHD010"
ENV{DEVTYPE}=="disk|partition", ENV{ID_SERIAL}=="WDC_WD40EZRZ-00WN9B0_WD-WCC4E4JUY3FC", ENV{ID_GWID}="GWHD011"

ENV{DEVTYPE}=="disk", ENV{ID_GWID}=="?*", SYMLINK+="disk/by-gwid/$env{ID_GWID}"
ENV{DEVTYPE}=="partition", ENV{ID_GWID}=="?*", SYMLINK+="disk/by-gwid/$env{ID_GWID}-part%n"
